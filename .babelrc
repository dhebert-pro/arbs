{
  "env": {
    "development": {
      "presets": "next/babel",
      "plugins": [
        ["module-resolver", {
          "root": ["./src"]
        }],
        ["inline-import", {
          "extensions": [".css"]
        }]
      ]
    },
    "production": {
      "presets": "next/babel",
      "plugins": [
        ["module-resolver", {
          "root": ["./src"]
        }],
        ["inline-import", {
          "extensions": [".css"]
        }]
      ]
    },
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "next/babel"
      ],
      "plugins": [
        ["module-resolver", {
          "root": ["./src"]
        }]
      ]
    }
  } 
}